{
  "name": "resume",
  "version": "2.0.0",
  "description": "白舜的个人在线简历",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --open",
    "dev": "npm run start",
    "build": "cross-env NODE_ENV=production webpack",
    "build:pdf": "cross-env NODE_ENV=production PDF_VERSION=true webpack",
    "build:inline": "cross-env NODE_ENV=production webpack && node scripts/create-inline.js",
    "inline": "npm run build:inline",
    "pdf": "npm run build:pdf && node scripts/generate-pdf.js"
  },
  "keywords": [
    "resume",
    "portfolio",
    "frontend",
    "developer",
    "whidy"
  ],
  "author": "whidy",
  "license": "ISC",
  "dependencies": {
    "tailwindcss": "^3.4.0",
    "lucide": "^0.400.0",
    "simple-icons": "^11.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.3",
    "html-webpack-plugin": "^5.6.0",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "puppeteer": "^21.0.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
